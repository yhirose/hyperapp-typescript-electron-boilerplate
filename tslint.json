{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [false, "always-prefix"],
    "max-line-length": {
      "options": [
        120
      ]
    },
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}
